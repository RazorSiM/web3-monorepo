{
  "name": "example-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "with-env": "dotenv -e .env -e ../../.env --",
    "dev": "pnpm with-env vite",
    "build": "pnpm clean:dist && pnpm type-check && pnpm build-only",
    "preview": "vite preview",
    "test:unit": "pnpm with-env vitest",
    "build-only": "pnpm with-env vite build",
    "type-check": "pnpm with-env vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean:dist": "rm -rf ./dist"
  },
  "dependencies": {
    "@web3-monorepo/example-erc20": "workspace:*",
    "pinia": "^2.1.4",
    "viem": "^1.2.14",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.4.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "@wagmi/cli": "^1.3.0",
    "@web3-monorepo/eslint-config": "workspace:*",
    "jsdom": "^22.1.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.3",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.4"
  }
}
